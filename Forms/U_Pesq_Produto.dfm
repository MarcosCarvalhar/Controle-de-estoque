  inherited q_pesq_padrao: TFDQuery
    SQL.Strings = (
      'SELECT '
      '   P.ID_PRODUTO,'
      '   P.CADASTRO,'
      '   P.PRODUTO_DESCRICAO,'
      '   P.ID_FORNECEDOR,'
      '   P.VL_CUSTO,'
      '   P.VL_VENDA,'
      '   P.ESTOQUE,'
      '   P.ESTOQUE_MIN,'
      '   P.UNIDADE,'
      '   F.NOME'
      ' FROM PRODUTO P, FORNECEDOR F'
      ' WHERE '
      '     P.ID_FORNECEDOR = F.ID_FORNECEDOR'
      '  AND'
      '     P.ID_PRODUTO = :PID_PRODUTO')
    ParamData = <
      item
        Name = 'PID_PRODUTO'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
